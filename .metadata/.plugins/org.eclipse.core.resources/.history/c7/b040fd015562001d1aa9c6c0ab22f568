	static void BDOORGPM() {
		System.out.println(ConsoleColor.BLUE + "+---------------------------+" + "\n"
						 						   + "| 1. Login As BDO |" + "\n"
						 						   + "| 2. Login As GPM |" + "\n"
						 						   + "| 3. Exit                   |" + "\n"
						 						   + "+---------------------------+" +ConsoleColor.RESET);
	}
	public static void main(String[] args) {
		System.out.println( ConsoleColor.BANANA_YELLOW+  "::::WELCOME To Mahatma Gandhi National Rural Employment Guarantee Act Application::::"+ConsoleColor.BLACK_BACKGROUND_BRIGHT);
		
		BDOORGPM();
	
	}
	
	
	
	

	

}
